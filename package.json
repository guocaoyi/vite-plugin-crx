{
  "name": "vite-plugin-crx",
  "version": "0.0.1",
  "description": "Vite plugin for Chrome Extensions",
  "author": "yalda",
  "license": "MIT",
  "keywords": [
    "vite",
    "vite-plugin",
    "chrome",
    "chrome-extension",
    "crx"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "types/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guocaoyi/vite-plugin-crx.git"
  },
  "bugs": {
    "url": "https://github.com/guocaoyi/vite-plugin-crx/issues"
  },
  "homepage": "https://github.com/guocaoyi/vite-plugin-crx#readme",
  "scripts": {},
  "dependencies": {
    "@babel/core": "^7.15.8"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.14",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.7.4",
    "vite": "^2.6.7"
  },
  "peerDependencies": {
    "@babel/core": "7.x",
    "vite": "2.x || 3.x"
  },
  "packageManager": "pnpm@7.4.0",
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write '**/*.{js,mjs,ts,md}' --cache --ignore-unknown"
    ],
    "src/*.ts": [
      "eslint --cache --fix"
    ]
  }
}
